@{
    ViewBag.Title = "AllMemberRecord";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}

<div ng-controller="AllMemberRecordCtrl" class="container" style="margin-top: 100px">
    <div class="row">
        <div class="col-lg-2 col-sm-2" ng-repeat="member in GetAllMember">
            <img src="~/images/Member/{{member.Name}}.jpg" class="img-thumbnail" alt="{{member.Name}}">
            <input type="submit" value={{member.Name}} ng-click="PersonalRecord(member.Account)" class="btn btn-warning" />
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/Scripts/WebApp/Member/AllMemberRecordCtrl.js"></script>
}
